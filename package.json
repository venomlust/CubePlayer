  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/venomlust/CubePlayer/issues"
  },
  "homepage": "https://github.com/venomlust/CubePlayer.git#readme",
  "devDependencies": {
    "electron-packager": "^5.2.1",
    "electron-prebuilt": "^0.37.2"
  },
  "dependencies": {
    "configstore": "^2.0.0",
    "howler": "^1.1.29",
    "musicmetadata": "^2.0.2",
    "uuid": "^2.0.1"
